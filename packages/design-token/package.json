{
	"name": "@exem-fe/design-token",
	"version": "1.1.2",
	"description": "EXEM Design System - Design Tokens",
	"author": "EXEM Design Team",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./css": "@exem-fe/stylesheet"
	},
	"sideEffects": false,
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"generate": "tsx src/generateToken.ts"
	},
	"keywords": [
		"design-token",
		"exem",
		"design-system",
		"typescript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/EXEM-FE/design-system",
		"directory": "packages/design-token"
	},
	"homepage": "https://github.com/EXEM-FE/design-system",
	"bugs": {
		"url": "https://github.com/EXEM-FE/design-system/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"tsup": "^8.5.0",
		"tsx": "^4.0.0",
		"typescript": "^5.3.3"
	}
}

{
	"name": "exem-design-token",
	"version": "0.2.1",
	"description": "EXEM Design System - Design Tokens",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"generate": "tsx src/generateToken.ts"
	},
	"keywords": [
		"design-token",
		"exem",
		"design-system"
	],
	"devDependencies": {
		"tsx": "^4.0.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	}
}

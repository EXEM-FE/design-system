{
  "name": "@exem-fe/tailwindcss-plugin",
  "version": "1.0.6",
  "description": "EXEM Design System - Tailwind CSS 3 Plugin",
  "author": "EXEM Design Team",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "tailwindcss",
    "tailwindcss3",
    "plugin",
    "design-system",
    "exem",
    "css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EXEM-FE/design-system",
    "directory": "packages/tailwindcss3-plugin"
  },
  "homepage": "https://github.com/EXEM-FE/design-system",
  "bugs": {
    "url": "https://github.com/EXEM-FE/design-system/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@exem-fe/design-token": "workspace:*",
    "@exem-fe/stylesheet": "workspace:*"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.18",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  }
}
